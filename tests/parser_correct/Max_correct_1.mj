class MyAdd {
	public int offset;
	public MyAdd MyAdd(int offset) {
		this.offset = offset;
	}

	public int add(int x) {
		return x + this.offset;
	}

	public int add(int x, int y) {
		return x + y + this.offset;
	}

	public int getOffset() {
		return this.offset;
	}
} 

class Main {
public static void main(String[] args) {
	int offset = 4*10;
	MyAdd ma = MyAdd(offset);
	/* is it possible to print strings in MiniJava?
	System.out.println("initialized MyAdd with offset: "+offset);
	System.out.println("MyAdd of 5 and 4 is: "+ma.add(5,4); */
	System.out.println(offset);
	System.out.println(ma.add(5,4));
	return 0;
}
}
