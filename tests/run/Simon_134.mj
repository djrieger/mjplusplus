class a {
	public int x;
	public static void main(String[] args) {
		new a().q();
	}
	
	public void q() {
		boolean y = impure() || impure();
		print(y);
		System.out.println(x);
	}
	
	public void print(boolean b) {
		if (b) System.out.println(3); 
		else System.out.println(4);
	}
	
	public boolean impure() {
		x = x+1;
		return false;
	}	
}
