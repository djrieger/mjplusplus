class a {
	public static void main(String[] a)
	{
		new a().q();
	}
	
	public void q() {
		int q = 0;
		int i = 27;
		int iterations = 1000000000;
		while (q < iterations) {
			i = step(i); /* @comment "can be inlined" */
			q = q + 1;
		}
		System.out.println(i);
	}
	
	public int step(int x) {
		if (even(x))
			return half(x); /* @comment "can be inlined" */
		else
			return times3plus1(x); /* @comment "can be inlined" */
	}
	
	public boolean even(int x) {
		return isZero(mod2(x)); /* @comment "can be inlined" */
	}
	
	public boolean isZero(int x) {
		return eq(x,0); /* @comment "can be inlined" */
	}
	
	public boolean eq(int x, int y) {
		return x==y;
	}
	
	public int mod2(int x) {
		return x%2;
	}
	
	public int half(int x) {
		return x/2;
	}
	
	public int times3plus1(int x) {
		return inc(times3(x)); /* @comment "can be inlined" */
	}
	
	public int times3(int x) {
		return 3*x;
	}
	
	public int inc(int x) {
		return x+1;
	}
}
